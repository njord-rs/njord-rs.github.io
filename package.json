    "autoprefixer": "^10.4.21",
    "eslint": "^9.28.0",
    "eslint-plugin-svelte": "^3.9.1",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "svelte": "^5.36.0",
    "svelte-check": "^4.2.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
