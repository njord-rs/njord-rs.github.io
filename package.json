    "autoprefixer": "^10.4.21",
    "eslint": "^9.25.0",
    "eslint-plugin-svelte": "^3.5.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.25.9",
    "svelte-check": "^4.1.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vitest": "^3.1.2"
